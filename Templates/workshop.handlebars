<div class="container">
	<h2>{{Subject}}</h2>
	<h3>
		<a href="/Home/Participants#{{Instructor}}">{{Instructor}}</a>
	</h3>
	<div>
		<div class="row">
			<div>
				<h4>Place:</h4>
				<p>{{{Place}}}</p><br />
				<h4>Meets On</h4>
				<p>{{{MeetOn}}}</p><br />
				<h4>Workshop Presentation</h4>
				<p>{{{Definition}}}</p><br />
				{{#if Description}}
				<h4>Main Topic and Audience</h4>
				<p>
					{{{Description}}}
				</p>
				{{/if}}
				<br />
				<h4>Participation Rules</h4>
				<p>
					{{{Rules}}}
				</p>
				{{#if Help}}
				<br />
				<h4>Items Included</h4>
				<p>
					{{{Help}}}
				</p>
				{{/if}}
				{{#if RegisterUrl}}
				<br />
				<a href="{{RegisterUrl}}" class="btn btn-lg btn-primary">Enroll Now</a>
				{{/if}}
			</div>
		</div>
	</div>
</div>